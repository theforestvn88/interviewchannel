<% if @interview.errors.empty? %>
    <%= turbo_stream.replace "round#{@interview.id}" do %>
        <%= render partial: "interviews/round", locals: {round: @interview, user: current_user, timezone: current_user.curr_timezone} %>
    <% end %>
<% else %>
    <%= turbo_stream.replace "modal" do %>
        <%= render template: "interviews/edit", formats: [:html] %>
    <% end %>
<% end %>